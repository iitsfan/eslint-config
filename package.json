{
	"name": "@itsfan/eslint-config",
	"type": "module",
	"version": "1.0.4",
	"description": "FAN is eslint flat config.",
	"keywords": [
		"eslint-config"
	],
	"author": "FAN",
	"license": "MIT",
	"homepage": "https://github.com/iitsfan/eslint-config#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iitsfan/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/iitsfan/eslint-config/issues"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "pnpm dlx @eslint/config-inspector@latest --config eslint-inspector.config.ts",
		"build": "pnpm run build:typegen && tsdown",
		"build:typegen": "tsx scripts/typegen.ts",
		"build:inspector": "pnpm dlx @eslint/config-inspector@latest build --config eslint-inspector.config.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"packageManager": "pnpm@10.16.1",
	"peerDependencies": {
		"eslint": "^9.34.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@next/eslint-plugin-next": "^15.5.3",
		"@stylistic/eslint-plugin": "^5.3.1",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"eslint-flat-config-utils": "^2.1.1",
		"eslint-plugin-better-tailwindcss": "^3.7.8",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-jsdoc": "^57.0.7",
		"eslint-plugin-jsonc": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-n": "^17.22.0",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-unicorn": "^61.0.2",
		"eslint-plugin-unused-imports": "^4.2.0",
		"eslint-plugin-vue": "^9.33.0",
		"eslint-plugin-vuejs-accessibility": "^2.4.1",
		"globals": "^16.4.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^0.5.1",
		"typescript-eslint": "^8.43.0",
		"vue-eslint-parser": "^9.4.3"
	},
	"devDependencies": {
		"@types/eslint-plugin-jsx-a11y": "^6.10.0",
		"@types/node": "^24.4.0",
		"eslint": "^9.35.0",
		"eslint-typegen": "^2.3.0",
		"tsdown": "^0.14.2",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	}
}
